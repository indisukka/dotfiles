{
    "base16.generator.activatedThemes": [
        "base16-default-dark",
        "base16-solar-flare-dark",
        "base16-tomorrow-night-dark"
    ],
    "workbench.colorTheme": "Base16 Tomorrow Night Dark",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "'offlig','m plus code latin',monospace, 'symbols nerd font mono'",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "editor.lineHeight": 1.7,
    "editor.fontWeight": "400",
    "editor.mouseWheelZoom": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.fontFamily": "'ubuntu sans mono', monospace,'symbols nerd font mono'",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.external.linuxExec": "alacritty",
    "window.zoomLevel": 0.6,
    "editor.minimap.enabled": false,
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.profiles.linux": {
        "fish": {
            "path": "fish"
        },
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        }
    },
    "customizeUI.font.regular": "'inter'",
    "customizeUI.fontSizeMap": {
        "13px": "11px",
        "monospace": "12px"
    },
    "customizeUI.font.monospace": "'jetbrains mono'",
    "redhat.telemetry.enabled": false
}